{
  "presets": ["react", "es2015" , "stage-0"],
  "plugins": [
    "transform-object-rest-spread",
    "transform-runtime"
  ],
  "env": {
    "production": {
      "plugins": ["transform-react-remove-prop-types"]
    },
    "test": {
      "plugins": [
        ["babel-plugin-webpack-alias", {
          "config": "./webpack/hot.config.js"
        }]
      ]
    }
  }
}
