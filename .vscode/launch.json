{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Front Firefox",
            "type": "firefox",
            "request": "launch",
            "reAttach": true,
            "url": "https://argu.dev/nederland",
            "pathMappings": [{
                "url": "webpack:///",
                "path": "${webRoot}"
            }]
        },
        {
            "name": "Attach Chrome Front",
            "type": "chrome",
            "request": "attach",
            "url": "https://argu.dev/nederland",
            "port": 9222,
            "sourceMaps": true,
            "webRoot": "${workspaceFolder}",
            "sourceMapPathOverrides": {
                "webpack:///./*":"${workspaceFolder}/*"
            }
        }
    ]
}