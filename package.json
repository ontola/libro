  "scripts": {
    "build": "rimraf ./dist && yarn run build:bundle && yarn run build:server",
    "build:bundle": "NODE_ENV=production webpack -p --config ./webpack/static.config",
    "build:server": "webpack -p --config ./webpack/server.config.js",
    "build:verbose": "rimraf ./dist && NODE_ENV=production webpack --production --progress --verbose --display-error-details --config ./webpack/static.config",
    "corsproxy": "./node_modules/.bin/corsproxy",
    "coverage": "nyc -e .jsx yarn run test",
    "doc:style": "sassdoc ./app -d ./docs/sass",
    "dev": "npm-run-all -p corsproxy start styleguide-server",
    "lint": "./node_modules/.bin/eslint --ext .jsx,.js .",
    "lint:style": "sass-lint ./app/**/*.scss  -v -q",
    "secure": "nsp check && retire",
    "start": "rimraf ./dist && NODE_ENV=development yarn run build:server && ELASTICSEARCH_URL=http://localhost:1337/aod-search.argu.co/aod_search NODE_ENV=development node ./dist/server.js",
    "start:local-api": "rimraf ./dist && ARGU_API_EXT_BASE=http://localhost:3030/api/ NODE_ENV=development yarn run build:server && ARGU_API_EXT_BASE=http://localhost:3030/api/ ELASTICSEARCH_URL=http://localhost:1337/aod-search.argu.co/aod_search NODE_ENV=development node ./dist/server.js",
    "start:prod": "ARGU_API_URL=http://localhost:3030 NODE_ENV=production node ./dist/server.js",
    "stats:bundle": "NODE_ENV=production webpack -p --config ./webpack/static.config.js --json > stats.json",
    "styleguide-build": "styleguidist build",
    "styleguide-server": "styleguidist server",
    "test": "NODE_PATH=./app mocha ./tests/testhelper.js \"./app/**/*.spec.js\" --compilers js:babel-core/register",
    "test:nightwatch": "./node_modules/.bin/nightwatch",
    "test:nightwatch:prod": "./node_modules/.bin/nightwatch --env ci -t tests/index.js"
  },
  "moduleRoots": [
    "./app"
  ],
  "pre-commit": [
    "lint",
    "lint:style",
    "test"
  ],
  "nyc": {
    "include": [
      "app/**"
    ]
  }
}
