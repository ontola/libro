  "scripts": {
    "build": "rimraf ./dist/* && tsc -p app/tsconfig.json && yarn t10s:extract && yarn run build:bundle && yarn run build:server",
    "build:analyze": "cross-env NODE_ENV=production NODE_OPTIONS=--max_old_space_size=4096 webpack --progress --config ./webpack/static.config.js --profile --json > stats.json",
    "build:bundle": "cross-env NODE_ENV=production NODE_OPTIONS=--max_old_space_size=4096 webpack --progress --config ./webpack/static.config.js",
    "build:server": "webpack --config server.config.js",
    "build:verbose": "rimraf ./dist && NODE_ENV=production webpack -p --progress --verbose --display-error-details --config ./webpack/static.config.js",
    "coverage": "jest --coverage",
    "t10s:extract": "formatjs extract  'app/**/*[!\\.d].{js,jsx,ts,tsx}' --out-file app/translations/en.json --id-interpolation-pattern '[sha512:contenthash:base64:6]' --throws",
    "t10s:compile": "formatjs compile-folder --ast app/translations app/lang",
    "t10s:lint": "node --experimental-modules --harmony-top-level-await --experimental-json-modules ./bin/lint-translations.mjs",
    "lint": "yarn lint:code && yarn lint:style && yarn t10s:lint",
    "lint:code": "yarn lint:code:app && ./node_modules/.bin/eslint --ext .jsx,.js,.ts,.tsx .",
    "lint:code:app": "tsc -p app/tsconfig.json",
    "lint:style": "sass-lint ./app/**/*.scss -v -q",
    "start": "cross-env NODE_ENV=development yarn run build:server && yarn run t10s:compile && cross-env NODE_ENV=development node -r dotenv/config --use-openssl-ca ./dist/private/server.js",
    "standalone": "cross-env STANDALONE=true yarn run start",
    "start:prod": "cross-env NODE_ENV=production node --use-openssl-ca ./dist/private/server.js",
    "stats:bundle": "cross-env NODE_ENV=production webpack -p --config ./webpack/static.config.js --json > stats.json",
    "test": "NODE_ENV=test jest",
    "ts-compile-check": "tsc -p app/tsconfig.json --noEmit"
  },
