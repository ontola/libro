{
  "name": "argu-open-data",
  "version": "0.1.0",
  "description": "Een interface voor motiedata uit de gemeenteraad",
  "repository": {
    "type": "git",
    "url": "https://tom_dalenberg@bitbucket.org/arguweb/aod_demo.git"
