  "scripts": {
    "build": "rimraf ./dist/* && tsc && yarn build:translations && yarn run build:bundle && yarn run build:server",
    "build:analyze": "webpack -p --progress --verbose --display-error-details --config ./webpack/static.config --profile --json  > stats.json",
    "build:bundle": "cross-env NODE_ENV=production parallel-webpack -p --progress --config ./webpack/static.config",
    "build:server": "webpack --config server.config.js",
    "build:translations": "NODE_ENV=development extract-messages -l=en,nl -o app/translations -d en --flat true 'app/**/!(*.spec).{t,j}s{x,}'",
    "build:verbose": "rimraf ./dist && NODE_ENV=production webpack -p --progress --verbose --display-error-details --config ./webpack/static.config",
    "coverage": "jest --coverage",
    "doc:style": "sassdoc ./app -d ./docs/sass",
    "lint": "tsc && yarn lint:code && yarn lint:style && yarn lint:translations",
    "lint:code": "tsc && tslint --project tsconfig.json && ./node_modules/.bin/eslint --ext .jsx,.js .",
    "lint:style": "sass-lint ./app/**/*.scss -v -q",
    "lint:translations": "./bin/lint-translations.sh",
    "secure": "nsp check && retire",
    "start": "cross-env NODE_ENV=development yarn run build:server && cross-env NODE_ENV=development node -r dotenv/config --use-openssl-ca $NODE_DEBUG_OPTION ./dist/private/server.js",
    "start:prod": "cross-env NODE_ENV=production node --use-openssl-ca ./dist/private/server.js",
    "stats:bundle": "cross-env NODE_ENV=production webpack -p --config ./webpack/static.config.js --json > stats.json",
    "test": "NODE_ENV=test jest",
    "ts-compile-check": "tsc -p tsconfig.json --noEmit"
  },
