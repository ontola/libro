  "scripts": {
    "build": "rimraf ./dist/* && yarn t10s:extract && yarn run build:bundle && yarn run build:metaData",
    "build:analyze": "cross-env TEST_BUILD=true ANALYZE_BUNDLE=true NODE_ENV=production NODE_OPTIONS=--max_old_space_size=4096 webpack --progress --config ./webpack/static.config.js --profile --json > stats.json",
    "build:bundle": "cross-env NODE_ENV=production NODE_OPTIONS=--max_old_space_size=4096 webpack --progress --config ./webpack/static.config.js",
    "build:shared": "yarn run build:metaData && yarn run build:parseToGraph",
    "build:metaData": "esbuild --bundle --format=esm --outfile=./dist/metaData.js app/modules/Common/lib/metaData.ts",
    "build:parseToGraph": "esbuild --bundle --format=esm --outfile=./dist/parseToGraph.js app/modules/Studio/lib/parseToGraph.ts",
    "build:verbose": "rimraf ./dist && NODE_ENV=production webpack -p --progress --verbose --display-error-details --config ./webpack/static.config.js",
    "t10s:extract": "formatjs extract  'app/**/*[!\\.d].{js,jsx,ts,tsx}' --out-file app/translations/en.json --id-interpolation-pattern '[sha512:contenthash:base64:6]' --throws",
    "t10s:compile": "formatjs compile-folder --ast app/translations app/lang",
    "t10s:lint": "node --experimental-modules --harmony-top-level-await --experimental-json-modules ./bin/lint-translations.mjs",
    "lint": "yarn lint:code && yarn t10s:lint",
    "lint:code": "yarn lint:ts && ./node_modules/.bin/eslint --ext .jsx,.js,.ts,.tsx .",
    "lint:ts": "tsc --noEmit -p app/tsconfig.json",
    "start": "yarn run t10s:compile && yarn run start:wds",
    "start:wds": "cross-env NODE_ENV=development webpack serve -c ./webpack/hot.config.js",
    "start:wps": "cross-env NODE_ENV=production webpack serve -c ./webpack/hot.config.js",
    "standalone": "cross-env STANDALONE=true yarn run start",
    "test": "NODE_ENV=test jest"
  },
